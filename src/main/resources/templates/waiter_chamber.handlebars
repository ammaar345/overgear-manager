        <link rel="stylesheet" href="/css/style.css">

    </head>

    <style>
table, th, td{
border: 1px solid black;
}
table{
width:50%;
border-collapse: collapse;
}

body {
  margin: 0;
  font-family: "Lato", sans-serif;
}

.sidebar {
  margin: 0;
  padding: 0;
  width: 200px;
  background-color: #f1f1f1;
  position: fixed;
  height: 100%;
  overflow: auto;
}

.sidebar a {
  display: block;
  color: black;
  padding: 16px;
  text-decoration: none;
}

.sidebar a.active {
  background-color: #04AA6D;
  color: white;
}

.sidebar a:hover:not(.active) {
  background-color: #555;
  color: white;
}

div.content {
  margin-left: 200px;
  padding: 1px 16px;
  height: 1000px;
<!--  float: none;-->
}

@media screen and (max-width: 700px) {
  .sidebar {
    width: 100%;
    height: auto;
    position: relative;
  }
  .sidebar a {float: left;}
  div.content {margin-left: 0;}
}

@media screen and (max-width: 400px) {
  .sidebar a {
    text-align: center;
    float: none;
  }
}
</style>

    <div class="container bg-faded">
        <h1 style="color: rgb(10, 134, 156);" class="text-center">Welcome To The Overgeared Restaurant Waiter</h1>

        <div class="row">

        </div>
        <hr>


            <div class="sidebar">
                <header id="header">THE OVERGEARED RESTAURANT WAITER CHAMBERS</header>
                <a class="" href="/waiter_chamber">ROSTER SCHEDULE</a>
                <a href="/waiterAbsence" class="nav-link">ABSENCE REPORT</a>
                <a href="/" class="nav-link">HOME</a>

            </div>

            <div class="content">

                <header>ROSTER SCHEDULE</header>

                <table class=" table table-bordered text-dark">
                    <!--<tr>-->
                    <tr>

                        {{#each days}}
                            <th>
                                {{this}}

                            </th>
                        {{/each}}

                    </tr>


                    <tr>

                        <td scope="col">
                            {{#each waiters1}}
                                <li>{{this}}</li>

                            {{/each}}

                        </td>
                        <td scope="col">
                            {{#each waiters2}}
                                <li>{{this}}</li>

                            {{/each}}

                        </td>
                        <td scope="col">
                            {{#each waiters3}}
                                <li>{{this}}</li>

                            {{/each}}

                        </td>
                        <td scope="col">

                            {{#each waiters4}}
                                <li>{{this}}</li>

                            {{/each}}

                        </td>
                        <td scope="col">

                            {{#each waiters5}}
                                <li>{{this}}</li>

                            {{/each}}

                        </td>
                        <td scope="col">
                            {{#each waiters6}}
                                <li>{{this}}</li>

                            {{/each}}

                        </td>
                        <td scope="col">

                            {{#each waiters7}}
                                <li>{{this}}</li>

                            {{/each}}

                        </td>
                    </tr>
                    </tbody>

                </table>
            </div>
        </form>/
    </div>
    </body>
</html>