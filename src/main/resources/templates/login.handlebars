<body>
<div class="container">
    <center>
        <div class="col-3">
            <form class="login-form" action="/login" method="POST">
                <h1 class="h4 mb-3 font-weight-normal">overgeared.Manager Login Form</h1>
              <label for="" class="email">Email: </label>
               <input type="" name="email" id="inputMail" class="form-email" placeholder="Example@me.com" required autofocus>
                <br>
                <br>
              <label for="inputPassword" class="password">Password: </label>
               <input type="password" name="password" id="inputPassword" class="form-control" placeholder="Password" required>
                <div class="checkbox mb-3">
                    <label>
                        <input type="checkbox" value="remember-me"> Remember me
                    </label>
                </div>
            </form>
<!--                <form action=/login/roster" method="POST">-->
            <a href="/login/roster"><button class="btn btn-lg btn-primary btn-block" type="submit">Login</button></a>


        </div>
    </center>
</div>


